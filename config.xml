<?xml version='1.0' encoding='utf-8'?>
<widget id="com.MarcosNieto.GuiaLaRioja" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Rioja Ciudad</name>
    <description>
       Guía La Rioja es un aplicación para asistir y ayudar a los vecinos de la Ciudad de La Rioja.
    </description>
    <author email="marcos.nieto17@gmail.com" href="http://www.unlarsomostodos.tk/">
        Marcos Nieto
    </author>
    <content src="index.html" />
    <access origin="*" />
   	
	
	<gap:splash src="splash.png" />
<preference name="show-splash-screen-spinner" value="true" />
<preference name="SplashScreenDelay" value="5000" />    
<preference name="fullscreen" value="true" />
	
<preference name="android-minSdkVersion" value="17" />)
<preference name="android-targetSdkVersion" value="28" />)


	
	<platform name="android">
        <icon qualifier="xxxhdpi" src="Icon.png" />
        <splash qualifier="" src="Splash.png" />
   	</platform>
	
	<platform name="ios">
	    <icon qualifier="xxxhdpi" src="Icon.png" />
        <splash qualifier="" src="Splash.png" />
		</platform>
	
	
	
<gap:plugin name="cordova-plugin-inappbrowser" source="npm"/>

<gap:plugin name="cordova-plugin-splashscreen" source="npm"/>
	
	
	
</widget>